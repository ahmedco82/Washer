<android.support.constraint.ConstraintLayout
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    tools:ignore="GradleDependency">
<ScrollView
    android:id="@+id/scroll"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    tools:layout_editor_absoluteY="0dp"
    tools:layout_editor_absoluteX="8dp"
    tools:ignore="MissingConstraints">

    <android.support.constraint.ConstraintLayout
        android:orientation="vertical"
        android:background="@drawable/bg_home"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:ignore="GradleDependency">


    <android.support.constraint.Guideline
        android:id="@+id/horizontalCenterGuideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.46"
        tools:layout_editor_absoluteY="264dp"
        tools:layout_editor_absoluteX="0dp"/>


    <android.support.constraint.Guideline
        android:id="@+id/verticalCenterGuideline2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="175dp"/>


    <android.support.constraint.Guideline
        android:id="@+id/verticalCenterGuideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="504dp"
        tools:layout_editor_absoluteY="25dp"
        tools:layout_editor_absoluteX="504dp" />
    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="أختر نوع الخدمة التي تريدها "
        android:textColor="#ffffff"
        android:textSize="21dp"
        android:textStyle="bold"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        tools:ignore="HardcodedText,SpUsage"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="150dp"/>

    <Button
        android:id="@+id/button"
        android:layout_width="74dp"
        android:layout_height="74dp"
        android:background="@drawable/b_home_a"
        tools:ignore="RtlHardcoded"
        app:layout_constraintTop_toTopOf="@+id/guideline"
        app:layout_constraintLeft_toLeftOf="@+id/guideline3"
        android:layout_marginEnd="63dp"
        android:layout_marginTop="36dp"
        android:layout_marginRight="32dp" />
    <Button
        android:id="@+id/button4"
        android:layout_width="74dp"
        android:layout_height="74dp"
        android:background="@drawable/b_home_b"
        app:layout_constraintTop_toTopOf="@+id/horizontalCenterGuideline"
        app:layout_constraintRight_toLeftOf="@+id/verticalCenterGuideline2"
        tools:ignore="MissingConstraints,RtlHardcoded" />
    <Button
        android:id="@+id/button2"
        android:layout_width="74dp"
        android:layout_height="74dp"
        app:layout_constraintRight_toLeftOf="@+id/verticalCenterGuideline2"
        app:layout_constraintTop_toTopOf="@+id/guideline"
        android:background="@drawable/b_home_c"
        tools:ignore="RtlHardcoded"
        android:layout_marginRight="8dp"
        android:layout_marginTop="36dp"
        android:layout_marginEnd="8dp"/>
    <Button
        android:id="@+id/button3"
        android:layout_width="74dp"
        android:layout_height="74dp"
        android:background="@drawable/b_home_d"
        tools:ignore="MissingConstraints,RtlHardcoded"
        app:layout_constraintTop_toTopOf="@+id/horizontalCenterGuideline"
        app:layout_constraintRight_toLeftOf="@+id/verticalCenterGuideline"
        android:layout_marginTop="0dp"
        android:layout_marginRight="80dp"/>
        <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline"
        android:orientation="horizontal"
        app:layout_constraintGuide_end="181dp"/>

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline3"
        app:layout_constraintGuide_begin="286dp"
        android:orientation="vertical"/>
    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline2"
        app:layout_constraintGuide_begin="472dp"
        android:orientation="horizontal"
        tools:layout_editor_absoluteY="497dp"
        tools:layout_editor_absoluteX="0dp"/>
        <ImageView
            android:id="@+id/imageView78"
            android:layout_width="84dp"
            android:layout_height="76dp"
            android:background="@drawable/board_home"
            tools:layout_editor_absoluteY="0dp"
            android:layout_marginRight="0dp"
            tools:ignore="ContentDescription,RtlHardcoded"/>
            <!--
        <include
            android:id="@+id/flight_info"
            android:layout_alignParentBottom="true"
            layout="@layout/inputstexts_adddeaitels"
            android:layout_width="wrap_content"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_height="wrap_content"/>
            -->
</android.support.constraint.ConstraintLayout>
    </ScrollView>
    <android.support.v7.widget.Toolbar
        android:id="@+id/toolbar2"
        android:layout_width="0dp"
        android:layout_height="80dp"
        android:background="#ed3851"
        app:popupTheme="@style/AppTheme.PopupOverlay"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="RtlHardcoded"
        android:gravity="center"
        app:layout_constraintHorizontal_bias="0.554">
    </android.support.v7.widget.Toolbar>

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="84dp"
        android:layout_height="76dp"
        android:background="@drawable/logo"
        app:layout_constraintRight_toLeftOf="@+id/guideline34"
        tools:ignore="ContentDescription,MissingConstraints,RtlHardcoded"
        tools:layout_editor_absoluteY="0dp"
        android:layout_marginRight="0dp"/>

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline34"
        app:layout_constraintGuide_begin="321dp"
        android:orientation="vertical"
        tools:layout_editor_absoluteY="25dp"
        tools:layout_editor_absoluteX="321dp"/>

</android.support.constraint.ConstraintLayout>
